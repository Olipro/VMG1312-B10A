#: iostat.c:81 mpstat.c:83 pidstat.c:77 sar.c:88
#, c-format
msgid "Usage: %s [ options ] [ <interval> [ <count> ] ]\n"
msgstr "Penggunaan: %s [ opsi ] [ <interval> [ <hitungan> ] ]\n"

#: iostat.c:84
#, c-format
msgid ""
"Options are:\n"
"[ -c ] [ -d ] [ -N ] [ -n ] [ -h ] [ -k | -m ] [ -t ] [ -V ] [ -x ]\n"
"[ <device> [...] | ALL ] [ -p [ <device> [,...] | ALL ] ]\n"
msgstr ""
"Opsinya adalah:\n"
"[ -c ] [ -d ] [ -N ] [ -n ] [ -h ] [ -k | -m ] [ -t ] [ -V ] [ -x ]\n"
"[ <divais> [...] | ALL ] [ -p [ <divais> [,...] | ALL ] ]\n"

#: mpstat.c:86
#, c-format
msgid ""
"Options are:\n"
"[ -A ] [ -I { SUM | CPU | ALL } ] [ -u ] [ -P { <cpu> [,...] | ALL } ] [ -V ]\n"
msgstr ""
"Opsinya adalah:\n"
"[ -A ] [ -I { SUM | CPU | ALL } ] [ -u ] [ -P { <cpu> [,...] | ALL } ] [ -V ]\n"

#: mpstat.c:457 pidstat.c:1683 sar.c:350
msgid "Average:"
msgstr "Rata-rata:"

#: mpstat.c:769
#, c-format
msgid "Not that many processors!\n"
msgstr "Tidak sebanyak itu prosesor!\n"


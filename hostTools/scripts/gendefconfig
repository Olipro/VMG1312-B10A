   SEDCMD="$SEDCMD -e 's/# CONFIG_USB_ACM is not set/CONFIG_USB_ACM=m/'"
   SEDCMD="$SEDCMD -e 's/# CONFIG_USB_USBNET is not set/CONFIG_USB_USBNET=m/'"

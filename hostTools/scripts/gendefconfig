#if [ "$BRCM_CHIP" = "63268" -o "$BRCM_CHIP" = "6368" ]; then
driver_setup "EXT_BONDING" "EXT_BONDING"
#fi

driver_setup "ETHERNET" "ENET"

if [ "$BUILD_11ac_throughput_patch_from_412L07" != "" ]; then

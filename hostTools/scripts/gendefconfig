   SEDCMD="$SEDCMD -e 's/# CONFIG_RFKILL is not set/CONFIG_RFKILL=m/'"
   SEDCMD="$SEDCMD -e 's/# CONFIG_USB_HSO is not set/CONFIG_USB_HSO=m/'"
   SEDCMD="$SEDCMD -e 's/# CONFIG_USB_WDM is not set/CONFIG_USB_WDM=m/'"
   SEDCMD="$SEDCMD -e 's/# CONFIG_USB_NET_CDC_NCM is not set/CONFIG_USB_NET_CDC_NCM=m/'"

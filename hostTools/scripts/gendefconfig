   SEDCMD="$SEDCMD -e 's/# CONFIG_USB_SERIAL is not set/CONFIG_USB_SERIAL=m/'"
   SEDCMD="$SEDCMD -e 's/# CONFIG_USB_SERIAL_GENERIC is not set/CONFIG_USB_SERIAL_GENERIC=y/'"

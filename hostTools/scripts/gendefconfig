if [ "$BRCM_DRIVER_ARL" != "" ]; then
if [ "$BRCM_CHIP" = "63268" -o "$BRCM_CHIP" = "6362" -o "$BRCM_CHIP" = "6328" -o "$BRCM_CHIP" = "6318" ]; then
    SEDCMD="$SEDCMD -e 's/# CONFIG_BCM_ARL is not set/CONFIG_BCM_ARL=y/'"

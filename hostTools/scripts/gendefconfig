    SEDCMD="$SEDCMD -e 's/# CONFIG_BCM_BPM is not set/CONFIG_BCM_BPM=m/'"
    SEDCMD="$SEDCMD -e 's/# CONFIG_BCM9"$BRCM_CHIP"_BPM_IMPL/CONFIG_BCM_BPM_IMPL/'"    

if [ "$BRCM_CHIP" = "63268" -o "$BRCM_CHIP" = "6362" -o "$BRCM_CHIP" = "6328" -o "$BRCM_CHIP" = "6368" -o "$BRCM_CHIP" = "6816" -o "$BRCM_CHIP" = "6318" ]; then

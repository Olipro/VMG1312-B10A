  wlctl -i wl1  bi 100
  wlctl -i wl1  bcn_rotate 1
  wlctl -i wl1 frameburst 1
  wlctl -i wl1 ap_isolate 0
  wlctl -i wl1 pwr_percent 100
  wlctl -i wl1 wmf_bss_enable 0
  wlctl -i wl1.1 wmf_bss_enable 0

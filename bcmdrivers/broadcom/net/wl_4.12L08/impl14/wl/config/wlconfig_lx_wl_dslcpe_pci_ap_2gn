include $(WLCFGDIR)/wlconfig_lx_wl_dslcpepcisdcfg
include $(WLCFGDIR)/wlconfig_lx_wl_dslcpe

WL11AC=0

# features
#supported chips 4318B0 4321(x) 4322(x) 43222(x) 43112 6362 4313 43224 43225 43226
SUPPORTED_CHIPS := 4318 4321 4322 43222 43112 6362 4313 43224 43225 43226 43227 43228 4331

# D11 Core Revs 																		
# 9 (4318b0), 11 (4321a1), 
# 12 (4321b/c),
# 15 (4312), 16 (4322/43222/43112), 18 (43224a0), 
# 22 (6362/43226) 23 (43224b0),
# 24 (4313),  26 (4331a0),      
# 29 (4331b0), 30(43228),                                   

WLFLAGS += -DD11CONF=0x65c59a00 
WLFLAGS += -DACONF=0x40
WLFLAGS += -DGCONF=0x00000080
WLFLAGS += -DNCONF=0x0000101ff
WLFLAGS += -DLPCONF=0x2
WLFLAGS += -DD11CONF2=0x0